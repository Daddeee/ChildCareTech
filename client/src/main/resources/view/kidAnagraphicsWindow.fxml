<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0"
      prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="ChildCareTech.controller.KidAnagraphicController">
    <children>
        <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0"
              prefWidth="100.0">
            <children>
                <Button fx:id="addButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                        minWidth="-Infinity" mnemonicParsing="false" onAction="#addButtonAction" prefHeight="80.0"
                        prefWidth="80.0" text="ADD">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="backButton" onAction="#backButtonAction" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="80.0"
                        prefWidth="80.0" text="&lt;--">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="logOutButton" layoutX="20.0" layoutY="110.0" maxHeight="-Infinity" maxWidth="-Infinity"
                        minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="80.0"
                        prefWidth="80.0" text="LOGOUT">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
        <TableView fx:id="personTable" maxHeight="-Infinity" maxWidth="-Infinity" minWidth="-Infinity"
                   prefHeight="680.0" prefWidth="880.0">
            <columns>
                <TableColumn fx:id="lastNameColumn" prefWidth="75.0" text="Last Name"/>
                <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="First Name"/>
                <TableColumn fx:id="fiscalCodeColumn" prefWidth="75.0" text="Fiscal Code"/>
                <TableColumn fx:id="bDateColumn" prefWidth="75.0" text="Birth Date"/>
                <TableColumn fx:id="addressColumn" prefWidth="75.0" text="Address"/>
            </columns>
            <HBox.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </HBox.margin>
        </TableView>
    </children>
</HBox>
