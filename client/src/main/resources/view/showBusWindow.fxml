<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Label?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="ChildCareTech.controller.ShowBusController"
      prefHeight="600.0" prefWidth="600.0">
    <HBox>
        <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
               prefHeight="25.0" prefWidth="100.0" text="Targa:">
        </Label>
        <Label fx:id="licensePlateLabel" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
               prefHeight="25.0" prefWidth="100.0">
        </Label>
    </HBox>
    <HBox>
        <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
               prefHeight="25.0" prefWidth="100.0" text="Capienza:">
        </Label>
        <Label fx:id="capacityLabel" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
               prefHeight="25.0" prefWidth="100.0">
        </Label>
    </HBox>
</VBox>
